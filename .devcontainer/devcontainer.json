{
  "name": "SurDur",
  "dockerComposeFile": "../requirements/docker-compose.yml",
  "service": "backend",
  "workspaceFolder": "/app",
  "customizations": {
    "vscode": {
      "settings": {
        "python.pythonPath": "/opt/conda/envs/surdur-env/bin/python",
        "python.linting.enabled": true,
        "python.linting.pylintEnabled": true,
        "files.autoSave": "onFocusChange"
      },
      "extensions": [
        "ms-python.python",
        "esbenp.prettier-vscode",
        "dbaeumer.vscode-eslint"
      ]
    }
  },
  "overrideCommand": false,
  "portsAttributes": {
    "8000": {
      "label": "Backend (FastAPI)"
    }
  }
}