{
  "name": "My Project",
  "dockerComposeFile": "../requirements/docker-compose.yml",
  "service": "surdur-container",
  "workspaceFolder": "/app",
  "customizations": {
    "vscode": {
      "settings": {
        "python.pythonPath": "/opt/conda/envs/surdur-env/bin/python",
        "python.linting.enabled": true,
        "python.linting.pylintEnabled": true
      },
      "extensions": [
        "ms-python.python",
        "esbenp.prettier-vscode",  // Add any other extensions you use
        "dbaeumer.vscode-eslint"
      ]
    }
  }
}
